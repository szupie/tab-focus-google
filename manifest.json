{
	"manifest_version": 2,
	"name": "Tab Focus through Google Search Results",
	"version": "0.2.1",
	"description": "Use the tab key to navigate through Google search results",
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"include_globs": ["http://www.google.*/search*", "https://www.google.*/search*"],
			"js": ["dom-modifier.js"],
			"css": ["style.css"],
			"run_at": "document_end"
		}
	],
	"icons": {
		"48": "icon.png",
		"96": "icon@2x.png"
	}
}